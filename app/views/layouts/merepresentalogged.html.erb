<!doctype html>
<html>
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta name="og:title" content="Encontre candidaturas para vereador/a que te representem!">
    <meta name="og:description" content="Você pode mudar a cara da Câmara Municipal da sua cidade. Conheça o Me Representa!">
    <meta name="og:image" content="https://s3.amazonaws.com/hub-central/uploads/1473183975_thumb_Meme_Minha_Sampa_1024.jpg">
    <link rel="stylesheet" href="/assets/MyFontsWebfontsKit.css">
    <link rel="stylesheet" href="/assets/bootstrap.min.css">
    <link rel="stylesheet" href="/assets/merepresenta.css">
    <link rel="stylesheet" href="/assets/merepresentalogged.css">
    <%= yield :head %>
</head>
<body>
    <header>
        <nav class="navbar navbar-default">
           <div class="nav navbar-nav">
            <div class="container-fluid">
                <div class="dropdown">
                    <button id="dLabel" type="button" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false" class="main-menu-item">
                       <img class="user-image" src="<%= @current_user.picture %>" alt="Foto de <%= @current_user.name %>">
                        <span class="user"><%= (@current_user||@user).name %></span>
                        <span class="caret"></span>
                    </button>
                    <ul class="dropdown-menu" aria-labelledby="dropdownMenu1">
                        <%= yield :menu %>
                        <li>
                            <a href="#" id="sair" class="dropdown-item">Sair</a>
                            <%= form_tag(session_path(session[:user_id]), method: :delete) do %><% end %>
                        </li>
                    </ul>
                </div>
            </div>
               
           </div>
        </nav>
    </header>
    <main>
        <%= yield :main %>
    </main>
    <footer>
        <%= yield :footer %>
    </footer>
    <script src="/assets/jquery-3.1.0.min.js"></script>
    <script src="/assets/jquery.mask.min.js"></script>
    <script src="/assets/tether.min.js"></script><!-- Tether for Bootstrap --> 
    <script src="/assets/bootstrap.min.js"></script>
    <script src="/assets/merepresenta.js"></script>
    <%= yield :scripts %>
    <script>
         (function(i,s,o,g,r,a,m){i['GoogleAnalyticsObject']=r;i[r]=i[r]||function(){
         (i[r].q=i[r].q||[]).push(arguments)},i[r].l=1*new Date();a=s.createElement(o),
         m=s.getElementsByTagName(o)[0];a.async=1;a.src=g;m.parentNode.insertBefore(a,m)
         })(window,document,'script','https://www.google-analytics.com/analytics.js','ga');

         ga('create', 'UA-26278513-33', 'auto');
         ga('send', 'pageview');
    </script>
</body>
</html>